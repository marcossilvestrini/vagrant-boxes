[req]
distinguished_name = req_distinguished_name
x509_extensions = v3_ca
prompt = no

[req_distinguished_name]
countryName             = BR
stateOrProvinceName     = Sao Paulo
localityName            = Sao Paulo 
organizationName        = Skynet Inc.
commonName              = rootca.skynet.com.br
